{
  "expo": {
    "name": "RateWatch",
    "slug": "ratewatch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ratewatch.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-ratewatch.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ratewatch.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ratewatch.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ratewatch.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "389ce0e6-3056-4a10-9dde-cdf834a9b653"
      }
    }
  }
}
